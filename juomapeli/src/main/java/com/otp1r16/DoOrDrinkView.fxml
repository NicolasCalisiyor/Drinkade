<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.otp1r16.DoOrDrinkController">
   <children>
      <AnchorPane prefHeight="687.0" prefWidth="800.0">
         <children>
            <Button fx:id="doButton" layoutX="171.0" layoutY="207.0" mnemonicParsing="false" onAction="#clickDo" prefHeight="202.0" prefWidth="459.0" text="Click to start the game" textFill="WHITE">
               <font>
                  <Font name="Bodoni MT" size="21.0" />
               </font></Button>
            <Button fx:id="drinkButton" layoutX="171.0" layoutY="409.0" mnemonicParsing="false" onAction="#clickDrink" prefHeight="202.0" prefWidth="459.0" textFill="WHITE">
               <font>
                  <Font name="Bodoni MT" size="23.0" />
               </font></Button>
            <Button fx:id="DoOrDrinkMenuButton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#backToMenu" text="Menu" textFill="WHITE">
               <font>
                  <Font size="16.0" />
               </font></Button>
            <Text fx:id="title" fill="WHITE" layoutX="157.0" layoutY="89.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Do Or Drink">
               <font>
                  <Font name="OCR A Extended" size="73.0" />
               </font>
            </Text>
            <Text fx:id="playerName" fill="WHITE" layoutX="237.0" layoutY="177.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Name:" textAlignment="CENTER" wrappingWidth="328.0">
               <font>
                  <Font name="OCR A Extended" size="53.0" />
               </font>
            </Text>
         </children>
      </AnchorPane>
   </children>
</VBox>
