<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane stylesheets="@LanguageSelectStyle.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.otp1r16.controller.LanguageSelectController">
   <top>
      <AnchorPane prefHeight="600.0" prefWidth="900.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="finnishButton" layoutX="174.0" layoutY="240.0" mnemonicParsing="false" onAction="#setFinnish" prefHeight="120.0" prefWidth="200.0">
               <font>
                  <Font size="27.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="OPEN_HAND" />
               </cursor></Button>
            <Button fx:id="englishButton" layoutX="516.0" layoutY="240.0" mnemonicParsing="false" onAction="#setEnglish" prefHeight="120.0" prefWidth="200.0" stylesheets="@LanguageSelectStyle.css">
               <font>
                  <Font size="27.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="OPEN_HAND" />
               </cursor></Button>
         </children>
      </AnchorPane>
   </top>
</BorderPane>
