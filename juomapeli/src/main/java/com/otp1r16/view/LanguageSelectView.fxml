<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane stylesheets="@MenuScreen.css" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.otp1r16.controller.LanguageSelectController">
   <top>
      <AnchorPane prefHeight="300.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="finnishButton" layoutX="69.0" layoutY="90.0" mnemonicParsing="false" onAction="#setFinnish" prefHeight="120.0" prefWidth="200.0" text="%Finnish">
               <font>
                  <Font size="27.0" />
               </font></Button>
            <Button fx:id="englishButton" layoutX="325.0" layoutY="90.0" mnemonicParsing="false" onAction="#setEnglish" prefHeight="120.0" prefWidth="200.0" text="%English">
               <font>
                  <Font size="27.0" />
               </font></Button>
         </children>
      </AnchorPane>
   </top>
</BorderPane>
